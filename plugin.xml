<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.qublish.backgroundClock" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>BackgroundClock</name>
	<description>Cordova Background Clock</description>
	<js-module name="BackgroundClock" src="www/BackgroundClock.js">
		<clobbers target="cordova.plugins.BackgroundClock" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="BackgroundClock">
				<param name="android-package" value="com.qublish.backgroundClock.BackgroundClock" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml">
		</config-file>
		<source-file src="src/android/BackgroundClock.java" target-dir="src/com/qublish/backgroundClock/BackgroundClock" />
	</platform>
</plugin>
